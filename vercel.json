{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/dist/index.js"
		},
		{
			"src": "/(.*\\.(js|css|png|jpg|jpeg|svg|ico))",
			"dest": "/dist/public/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/dist/public/index.html"
		}
	]
}
